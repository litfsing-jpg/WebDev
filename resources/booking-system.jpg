<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 800" width="1200" height="800">
  <rect width="1200" height="800" fill="#F8FAFC"/>
  <rect x="50" y="50" width="1100" height="700" fill="white" stroke="#E2E8F0" stroke-width="2" rx="10"/>

  <!-- Header -->
  <rect x="50" y="50" width="1100" height="80" fill="#DC2626" rx="10"/>
  <text x="600" y="100" font-family="Arial" font-size="28" font-weight="bold" fill="white" text-anchor="middle">
    Restaurant Booking System
  </text>

  <!-- Calendar Grid -->
  <g transform="translate(100, 180)">
    <rect x="0" y="0" width="140" height="120" fill="#FEF2F2" stroke="#F87171" rx="6"/>
    <text x="70" y="70" font-family="Arial" font-size="24" fill="#DC2626" text-anchor="middle">15:00</text>

    <rect x="160" y="0" width="140" height="120" fill="#FEF2F2" stroke="#F87171" rx="6"/>
    <text x="230" y="70" font-family="Arial" font-size="24" fill="#DC2626" text-anchor="middle">16:00</text>

    <rect x="320" y="0" width="140" height="120" fill="#FEF2F2" stroke="#F87171" rx="6"/>
    <text x="390" y="70" font-family="Arial" font-size="24" fill="#DC2626" text-anchor="middle">17:00</text>

    <rect x="480" y="0" width="140" height="120" fill="#FEF2F2" stroke="#F87171" rx="6"/>
    <text x="550" y="70" font-family="Arial" font-size="24" fill="#DC2626" text-anchor="middle">18:00</text>

    <rect x="640" y="0" width="140" height="120" fill="#FEF2F2" stroke="#F87171" rx="6"/>
    <text x="710" y="70" font-family="Arial" font-size="24" fill="#DC2626" text-anchor="middle">19:00</text>

    <rect x="800" y="0" width="140" height="120" fill="#FEF2F2" stroke="#F87171" rx="6"/>
    <text x="870" y="70" font-family="Arial" font-size="24" fill="#DC2626" text-anchor="middle">20:00</text>
  </g>

  <!-- Tables -->
  <text x="600" y="380" font-family="Arial" font-size="22" fill="#64748B" text-anchor="middle">
    Available Tables
  </text>

  <rect x="200" y="420" width="280" height="200" fill="#DCFCE7" rx="8"/>
  <text x="340" y="530" font-family="Arial" font-size="20" fill="#059669" text-anchor="middle">
    Table 1-5: Available
  </text>

  <rect x="520" y="420" width="280" height="200" fill="#FEE2E2" rx="8"/>
  <text x="660" y="530" font-family="Arial" font-size="20" fill="#DC2626" text-anchor="middle">
    Table 6-10: Booked
  </text>
</svg>
